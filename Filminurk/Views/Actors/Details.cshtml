@model Filminurk.Models.Actors.ActorsDetailsViewModel

<h1>Actor Details</h1>

<div>
	<h1>Actor Details</h1>
	<hr />
	<dl class="row">
		<div class="form-group row">
			<label asp-for="ActorID" class="col-sw-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.ActorID)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="FirstName" class="col-sw-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.FirstName)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="LastName" class="col-sw-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.LastName)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="NickName" class="col-sw-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.NickName)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="MoviesActedFor" class="col-sw-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.MoviesActedFor)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="Age" class="col-sw-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.Age)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="Gender" class="col-sw-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.Gender)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="ActorRegion" class="col-sw-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.ActorRegion)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="EntryCreatedAt" class="col-sw-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.EntryCreatedAt)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="EntryModifiedAt" class="col-sw-2 col-form-label"></label>
			@Html.DisplayFor(Model => Model.EntryModifiedAt)
		</div>
	</dl>
	<a asp-action="Index" asp-controller="Actors" class="btn btn-success">Cansel</a>
</div>